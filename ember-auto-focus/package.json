{
  "name": "@zestia/ember-auto-focus",
  "version": "5.1.0-1",
  "description": "An HTML autofocus solution for Ember apps",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "rollup --config",
    "deploy": "ember build --environment production && ember github-pages:commit --message \"Deploy gh-pages from commit $(git rev-parse HEAD)\" && git push origin gh-pages:gh-pages",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint '**/*.{css,scss}'",
    "lint:css:fix": "stylelint '**/*.{css,scss}' --fix",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "prepack": "rollup --config",
    "prettier": "prettier . --write",
    "release": "release-it",
    "start": "rollup --config --watch",
    "test": "echo 'A v2 addon does not have tests, run tests in test-app'",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zestia/ember-auto-focus.git"
  },
  "peerDependencies": {
    "ember-source": "^3.28.0 || ^4.0.0"
  },
  "engines": {
    "node": "14.* || 16.* || >= 18"
  },
  "author": "Zestia",
  "license": "MIT",
  "dependencies": {
    "@embroider/addon-shim": "^1.8.5",
    "ember-modifier": "^4.1.0"
  },
  "devDependencies": {
    "@zestia/eslint-config": "^4.0.18",
    "@zestia/prettier-config": "^1.0.9",
    "@zestia/stylelint-config": "^4.1.5",
    "@zestia/template-lint-config": "^6.0.3",
    "@babel/core": "^7.22.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.22.5",
    "@babel/runtime": "^7.22.5",
    "@embroider/addon-dev": "^3.1.1",
    "@rollup/plugin-babel": "^6.0.3",
    "rollup": "^3.24.1",
    "rollup-plugin-copy": "^3.4.0"
  },
  "keywords": [
    "ember-addon",
    "focus",
    "autofocus",
    "input",
    "textarea",
    "modifier"
  ],
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "app-js": {
      "./modifiers/auto-focus.js": "./dist/_app_/modifiers/auto-focus.js"
    },
    "main": "addon-main.cjs",
    "type": "addon",
    "version": 2
  },
  "bugs": {
    "url": "https://github.com/zestia/ember-auto-focus/issues"
  },
  "homepage": "https://github.com/zestia/ember-auto-focus",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js",
    "./addon-main.js": "./addon-main.cjs"
  },
  "files": [
    "addon-main.cjs",
    "dist"
  ]
}
