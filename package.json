{
  "name": "@zestia/ember-auto-focus",
  "version": "3.0.10",
  "description": "An HTML autofocus solution for Ember apps",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint": "npm run lint:js && npm run lint:hbs && npm run lint:css",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "lint:css": "stylelint '**/*.{css,scss}'",
    "prettier": "prettier '**/*.{js,json,css,scss,html,md}' --write",
    "start": "ember serve",
    "test": "ember test && npm run lint",
    "test:all": "ember try:each",
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zestia/ember-auto-focus.git"
  },
  "engines": {
    "node": "8.* || >= 10.*"
  },
  "author": "Zestia",
  "license": "MIT",
  "devDependencies": {
    "@ember/optional-features": "^1.1.0",
    "@zestia/ember-template-lint-plugin": "^2.0.4",
    "@zestia/eslint-config": "^2.0.31",
    "@zestia/prettier-config": "^0.0.6",
    "@zestia/stylelint-config": "^2.0.24",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.14.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-test-loader": "^2.2.0",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^5.3.0",
    "ember-source": "~3.14.1",
    "ember-source-channel-url": "^2.0.1",
    "ember-template-lint": "^1.8.1",
    "ember-try": "^1.3.0",
    "eslint": "^6.6.0",
    "loader.js": "^4.7.0",
    "prettier": "^1.19.1",
    "qunit-dom": "^0.9.2",
    "release-it": "^12.4.3",
    "sass": "^1.23.3",
    "stylelint": "^11.1.1"
  },
  "dependencies": {
    "@glimmer/component": "^1.0.0-beta.3",
    "@ember/render-modifiers": "^1.0.2",
    "ember-modifier": "^1.0.2",
    "ember-cli-babel": "^7.12.0",
    "ember-cli-htmlbars": "^4.0.5"
  },
  "keywords": [
    "ember-addon",
    "autofocus",
    "input",
    "textarea"
  ],
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "bugs": {
    "url": "https://github.com/zestia/ember-auto-focus/issues"
  },
  "homepage": "https://github.com/zestia/ember-auto-focus"
}
